<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" minHeight="-Infinity" minWidth="-Infinity" prefHeight="139.0" prefWidth="229.0" spacing="20.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.examen.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <HBox prefHeight="100.0" prefWidth="200.0">
      <children>
          <Button onAction="#pedido" text="Pedidos">
            <HBox.margin>
               <Insets right="20.0" />
            </HBox.margin></Button>
         <VBox prefHeight="200.0" prefWidth="100.0">
            <children>
               <DatePicker fx:id="inicio" promptText="Fecha inicio" />
               <DatePicker fx:id="fin" promptText="Fecha fin">
                  <VBox.margin>
                     <Insets top="10.0" />
                  </VBox.margin>
               </DatePicker>
            </children>
         </VBox>
      </children>
   </HBox>
   <HBox fx:id="contenedor" prefHeight="100.0" prefWidth="200.0">
      <children>
         <Button onAction="#juego" text="Juegos">
            <HBox.margin>
               <Insets right="10.0" />
            </HBox.margin></Button>
         <Button mnemonicParsing="false" onAction="#ayuda" text="Ayuda de usuario" />
      </children>
   </HBox>
</VBox>
